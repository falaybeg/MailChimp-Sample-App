@model MailChimpApp.Models.MemberModel

<a href="@Url.Action("GetAllMember","Home")" class="btn btn-primary">Get Subscriber List</a>
<br />

<br>
<div class="form-group">



    @using (Html.BeginForm("AddSubscribe", "Home", FormMethod.Post))
    {
        <input type="text" id="subscribe" name="email" />
        <input type="submit" value="Subscribe" />

        if (@Model != null)
        {
            <span>@Model.TextMessage</span>
        }
    }



</div>